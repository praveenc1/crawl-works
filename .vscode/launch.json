{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable",
            "cargo": {
                "args": [
                    "build",
                    "--bin=web_crawl",
                    "--package=web_crawl",
                    "--manifest-path=Cargo.toml"
                ],
                "filter": {
                    "kind": "bin"
                }
            },
            "program": "${workspaceFolder}/target/debug/web_crawl",
            "args": [],
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "cargo build"
        }
    ]
}